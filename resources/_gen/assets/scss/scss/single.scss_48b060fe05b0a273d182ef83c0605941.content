html,body,ul,ol{width:100%;height:100%;padding:0;margin:0;box-sizing:border-box}ul,ol{list-style:none}a{text-decoration:none}.item{width:80%;max-width:1000px;margin:20px auto;position:relative;display:flex}.item>article{flex:1}.item>article>img{max-width:680px;border-radius:10px}.item>aside{width:200px;margin-top:40px;padding-left:20px}nav#TableOfContents ul li{margin-bottom:10px}nav#TableOfContents ul li>a{width:100%;display:inline-block;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}nav#TableOfContents ul li>ul{width:calc(100% - 20px);margin-top:10px;margin-left:30px}.fix-content{position:fixed;top:20px}